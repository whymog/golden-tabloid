{
  "manifest_version": 2,
  "name": "Golden Tabloid",
  "author": "Nick Cummings",
  "version": "0.0.1",
  "description": "An extension to make reading Achewood easier",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "http://*.achewood.com/"
  ]
}
